{
  "manifest_version": 3,
  "name": "YT Open Video Alone",
  "description": "Open a youtube video without any additional URL params",
  "version": "1.0",
  "icons": {
    "16": "icons/128.png",
    "32": "icons/128.png",
    "48": "icons/128.png",
    "128": "icons/128.png"
  },
  "background": {
    "service_worker": "scripts/background.js"
  },
  "permissions": [
    "contextMenus",
    "tabs"
  ]
}